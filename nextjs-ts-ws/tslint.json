{
  "extends": [
    "tslint-config-airbnb",
    "tslint-react"
  ],
  "rules": {
    "align": [
      true,
      "parameters",
      "statements",
      "members"
    ],
    "semicolon": [
      true,
      "always",
      "ignore-interfaces"
    ],
    "import-name": [
      false
    ],
    "variable-name": [
      true,
      "ban-keywords"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": " d=\""
      }
    ],
    "ter-arrow-parens": false,
    "jsx-boolean-value": false,
    "no-unused-variable": [
      true,
      {
        "ignore-pattern": "^_"
      }
    ],
    "jsx-no-multiline-js": false,
    "no-duplicate-imports": false,
    "no-else-after-return": false,
    "strict-boolean-expressions": false
  }
}
